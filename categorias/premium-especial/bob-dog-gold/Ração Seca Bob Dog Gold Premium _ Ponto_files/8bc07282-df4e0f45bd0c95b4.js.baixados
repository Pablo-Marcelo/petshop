    display: flex;
    flex-direction: row;
    align-items: center;
    background: #fff;
    border-radius: 3px;
    width: 100%;
    height: 40px;
    font-size: 14px;
    z-index: 999;

    input {
      border: none;
      color: ${({theme:e})=>e.colors.supportGray[500]};
      padding: 0.5rem;
      padding-left: 1rem;
      outline: none;
      font-weight: normal;
      flex: 1;

      &::placeholder {
        color: ${({theme:e})=>e.colors.supportGray[500]};
      }
    }

    button {
      background: transparent;
      display: flex;
      justify-content: flex-end;
      border: none;
      outline: none;
      cursor: pointer;
      padding: 8px ${({theme:e})=>"casas-bahia"===e.name?"12px":"16px"};
    }
  }

  ${({theme:e})=>"casas-bahia"===e.name?o.css`
          form.search__form {
            height: 50px;
            border: 1px solid ${e.colors.supportGray[300]};

            input {
              color: ${e.colors.supportGray[800]};

              &::placeholder {
                color: ${e.colors.supportGray[800]};
              }
            }
          }
        `:null}

  ${({theme:e})=>"ponto-frio"===e.name?o.css`
          form.search__form {
            border-radius: 50px;

            input {
              background-color: transparent;
              padding: 14px 0;
              margin-left: 16px;
            }

            button {
              padding: 0;
              margin-right: 16px;
            }
          }
        `:null}
